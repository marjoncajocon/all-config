	main:
	gcc-core,gcc-g++,make,gdb,binutils,patch,
	diffutils,file,autoconf,automake,libtool,
	m4,libxcb-devel,libxcb-devel,libssl-devel,
	libcurl-devel,libpng-devel,libffi-devel,

	other required building: example(matplotlib,open-cv python)
	libjpeg-devel,
	libtiff-devel,
	libavcodec-devel
	
Repo:
	git
	

